<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />

	<entity name="Sequence" cachable="true">
		<string name="prefix" title="Prefix" required="true" />
		<string name="suffix" title="Suffix" />
		<integer name="padding" min="1" max="10" title="Padding" />
		<string name="nextNumber" formula="true" title="Next Number" />
		<many-to-one name="model" ref="com.axelor.meta.db.MetaModel"
			required="true" title="Model"/>
	</entity>

</domain-models>
